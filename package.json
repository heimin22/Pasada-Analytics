{
  "name": "pasada-analytics",
  "version": "1.0.0",
  "description": "QuestDB Analytics Service for Pasada Backend",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/server.ts",
    "postinstall": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analytics:weekly": "ts-node src/scripts/weeklyAnalyticsPush.ts",
    "analytics:backfill": "ts-node src/scripts/weeklyAnalyticsPush.ts --backfill",
    "analytics:migrate": "ts-node src/scripts/migrateToQuestDB.ts",
    "analytics:test": "ts-node src/scripts/testQuestDBConnection.ts",
    "cron:weekly": "node dist/scripts/weeklyAnalyticsPush.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "node-fetch": "^3.3.2",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "compression": "^1.7.4",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "morgan": "^1.10.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "analytics",
    "questdb",
    "time-series",
    "traffic-analytics",
    "pasada"
  ],
  "author": "Your Name",
  "license": "MIT"
}